{
  "name": "@telegram-web-app/react",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.10.0"
  },
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "typings": "./dist/types/typings/public.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --pretty --noEmit",
    "build:dev": "cross-env NODE_ENV=development tsx scripts/build.ts",
    "build:prod": "cross-env NODE_ENV=production tsx scripts/build.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "pnpm test --coverage"
  },
  "peerDependencies": {
    "@telegram-web-app/core": "*",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@telegram-web-app/core": "workspace:*",
    "@types/react": "18.2.0",
    "esbuild-plugin-d-ts-path-alias": "4.0.0",
    "react": "18.2.0"
  }
}
